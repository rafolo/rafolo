<!-- RK Titles> -->
<% provide(:title, t2('Mappoints')) %>
<% provide(:subtitle, t2('Points of your map')) %>
<!-- <RK Titles -->

<!--RK Navigation> -->
<% content_for :navigation do %>
    <!-- SPA Navigations -->
    <li class="cdrop active"><%= link_to "MP/Todo", "#/js" %></li>
    <li class="cdrop active"><%= link_to "MP/About", "#/about" %></li>
